package constructor;

public class Car1 {
	public int gas;
	public int speed;
	public String owner;
	public double mpg;
	public String color;
	
	public Car1(int g , int s , String o , double m , String c) {
		gas = g;
		speed = s;
		owner = o;
		mpg = m;
		color = c;
		System.out.println("자동차 생성 완료!");
		System.out.println("연료 : " + gas +
				           ", 속도 : " + speed +
				           ", 주인 : " + owner +
				           ", 연비 : " + mpg +
				           ", 색상 : " + color);
}
}