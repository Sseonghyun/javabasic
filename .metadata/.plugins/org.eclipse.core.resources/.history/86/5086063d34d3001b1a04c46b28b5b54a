package accessmodifier;

public class Main {

	public static void main(String[] args) {
		// 전사를 하나 만들고 사냥을 3번 하세요.
		// 사냥 3번 후에 전체 상태를 sysout을 이용해 출력해주세요.
		Warrior w1 = new Warrior("전사");
		w1.hunt();
		w1.hunt();
		w1.hunt();
		System.out.println("현재 체력 : " + w1.hp + "입니다.");
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();

	}

}
