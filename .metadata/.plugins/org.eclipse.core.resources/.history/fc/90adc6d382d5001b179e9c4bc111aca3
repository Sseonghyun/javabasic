package overloading;

public class OverTest2 {
	
	private String a;
	private int b;
	
	public OverTest2() {
		this.a = "0";
		this.b = 1;
	}
	public OverTest2(String a) {
		this.a = a;
		this.b = 3;
	}
	public OverTest2(int b) {
		this.a = "3";
		this.b = b;
	}
	public OverTest2(String a , int b) {
		this.a = a;
		this.b = b;
	}
	public void showData() {
		System.out.println("출력1 : ");
		System.out.println("출력2 : " + a);
		System.out.println("출력3 : " + b);
		System.out.println("출력4 : " + a + "," + b);
	}
}
