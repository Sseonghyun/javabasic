package structure;

public class Structure3 {
	
	public static void getInfo(Cat p) {
		System.out.println(p.name);
		System.out.println(p.age);
		System.out.println(p.breed);
		
	public static void main(String[] args) {
		Cat a = new Cat();
		a.name = "뱅갈";
		a.age = 1;
	    a.breed = "뱅갈캣";
	    
	    Cat b = new Cat();
	    b.name = "시안";
	    b.age = 3;
	    b.breed = "페르시안";
	    
	    getInfo(a);
	    getInfo(b);
	}

}
