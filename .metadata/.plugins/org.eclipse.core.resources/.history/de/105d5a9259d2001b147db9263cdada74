package constructor;

public class Main {

	public static void main(String[] args) {
		Car car1 = new Car(100, 0, "서성현");
		Car car2 = new Car(80, 0, "홍길동");
		// 자동차 정보 조회
		car1.getInfo();
		car1.accelSpeed();
		car1.breakSpeed();
		car2.getInfo();
		car2.accelSpeed();
		car2.breakSpeed();
		

	}

}
