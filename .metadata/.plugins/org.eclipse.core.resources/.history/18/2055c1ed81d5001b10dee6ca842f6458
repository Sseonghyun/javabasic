
public class OverTest {
	
	private String a;
	private int b;
	
	public OverTest() {
		this.a = "0";
		this.b = 1;
	}
	public OverTest(String a) {
		this.a = a;
		this.b = 3;
	}
	public OverTest(int b) {
		this.a = "3";
		this.b = b;
	}
	public OverTest(String a , int b) {
		this.a = a;
		this.b = b;
	}
	public void showData() {
		System.out.println("1번유형 : ");
		System.out.println("2번유형 : " + a);
		System.out.println("3번유형 : " + b);
		System.out.println("4번유형 : " + a + "," + b);
	}
}
